<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.ubo.master.sudoku">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/lse2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/lse2"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sudoku">
        <activity android:name="fr.ubo.master.sudoku.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="fr.ubo.master.sudoku.JeuActivity">
        </activity>
        <activity android:name="fr.ubo.master.sudoku.AboutActivity">
        </activity>
        <activity android:name="fr.ubo.master.sudoku.ChoixActivity" android:theme="@style/PopupTheme">
        </activity>
        <activity android:name="fr.ubo.master.sudoku.GridChoiceActivity">
        </activity>
    </application>

</manifest>